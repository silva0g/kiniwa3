  "<div class='row'>",
  "  <div class='col-md-4'><%= product.listing_name %></div>",
  "  <div class='col-md-3'><%= quantity %></div>",
  "   <div class='col-md-3'>â‚¬<%= product.price * quantity.to_i %></div>",
  "   <div class='col-md-2'>",
  "   <a href='/menus/1/remove_product?product_id=<%= product.id %>' data-remote='true'>X</a> </div>",
  "</div>"
].join("\n"));
  $(".added-products").append(f);
<% end %>
$("#product_id").val('');
